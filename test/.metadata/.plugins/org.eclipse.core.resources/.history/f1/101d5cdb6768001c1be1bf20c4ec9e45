import java.util.*;
import java.util.Arrays;
class GFG {
    public static int
    findDisappearedNumbers(int[] nums)
    {
        int n=nums.length;
        int sum=((n+1)*(n+2))/2;
        for(int i=0;i<n;i++)
          sum-=nums[i];
        return sum;
    }
    public static void main(String[] args)
    {
        int[] a = { 9, 18, 6, 3, 12 };
        System.out.println(findDisappearedNumbers(a));
    }
}
